<% content_for(:breadcrumb) do %>
  <%= link_to "#{@project.github_project_user}/#{@project.github_project_name}", project_path(@project) %> 
<% end %>

<div class='main'>
  <h3>Users</h3>
  <table>
    <thead>
      <tr>
        <td>Email</td>
        <td>Last Login</td>
      </tr>
    </thead>
    <tbody>
      <% @users.each do |user| %>
        <tr>
          <td><%= user.email %></td>
          <td><%= user.updated_at %>
        </tr>
      <% end %>
    </tbody>
  </table>
</div>